
var startnumber = 60;
var counterFunctionCall = 0;
var interval = 1000;
function func() {
    console.debug("counterFunctionCall " + counterFunctionCall + "\n");
    counterFunctionCall = counterFunctionCall + 1;
    var text = startnumber - counterFunctionCall;
    return text;
}

function resetTimer() {
    counterFunctionCall = 0;
    interval = 500;
    console.debug("counterFunctionCall " + counterFunctionCall + "\n");
}

